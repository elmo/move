<%= turbo_stream.append "prices" do %>
  <%= turbo_frame_tag dom_id(@price) do %>
    <%= render "admin/commerce/prices/price",
                price: @price,
                product: @product %>
  <% end %>
<% end %>

<%= turbo_stream.update "new_price" do %>
  <%= render "admin/commerce/prices/add_price_button" %>
<% end %>
